CREATE TABLE RECS(ACTION TEXT NOT NULL,
		COMPANY_NAME TEXT NOT NULL,
		SYMBOL TEXT NOT NULL,
		TARGET REAL NOT NULL,
		RECOMMENDER TEXT NOT NULL,
		REC_DATE TEXT NOT NULL,
		DATE_ON_MC TEXT NOT NULL,
		PRICE_ON_REC_DATE REAL NOT NULL,
		PERF_TO_DATE REAL,
		TARGET_ACHIEVED INTEGER,
		ACHIEVED_DATE TEXT,
		ACHIEVED_IN_DAYS INTEGER,
		BEST_PRICE_DATE REAL,
		BEST_PERF REAL,
		BEST_PERF_ACHIEVED_DATE TEXT,
		URL TEXT,
		PRIMARY KEY (ACTION, SYMBOL, TARGET, RECOMMENDER, REC_DATE)
		);